	}
		
	System.out.println(p);

	 List < Collaborateur > tabCollab = new ArrayList <Collaborateur>();
	 tabCollab.equals(p);
	 


	        
        this.getServletContext().getRequestDispatcher("/WEB-INF/accueil.jsp").forward(request, response);
	}

}
